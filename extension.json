{
	"name": "IFrameTag",
	"version": "0.2",
	"author": [
		"[//hexmode.com Mark A. Hershberger]"
	],
	"descriptionmsg": "iframe-desc",
	"requires": {
		"MediaWiki": ">= 1.34.0",
		"platform": {
			"php": ">= 7.3"
		}
	},
	"type": "parserhook",
	"license-name": "GPL-3.0+",
	"MessagesDirs": {
		"IFrame": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\IFrame\\": "src/"
	},
	"Hooks": {
		"ParserFirstCallInit": "MediaWiki\\Extension\\IFrame\\Handler::register"
	},
	"config_prefix": "iFrame",
	"config": {
		"Domains": {
			"value": [],
			"descriptionmsg": "iframe-config-hosts",
			"path": false,
			"public": true
		},
		"OnWikiConfig": {
			"value": true,
			"descriptionmsg": "iframe-config-onwikiconfig",
			"path": false,
			"public": true
		}
	},
	"manifest_version": 2
}
